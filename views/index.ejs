<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <div id="app">
        {{ message }}
    </div>

    <h1>Repos</h1>
    <h1>Stars</h1>
    <h1>Tags</h1>
        <% repos.forEach(function(repo){ %>
           <li><%= repo.name %> | <%= repo.description %></li>
        <% }) %>

    <script src="https://cdn.jsdelivr.net/npm/vue"></script>
    <script>
        // 这是我们的Model
        var exampleData = {
            message: 'Hello World!'
        }

        // 创建一个 Vue 实例或 "ViewModel"
        // 它连接 View 与 Model
        new Vue({
            el: '#app',
            data: exampleData
        })
    </script>
  </body>
</html>
